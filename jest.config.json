{
    "globals": {},
    "moduleNameMapper": {},
    "moduleDirectories": [
        "src",
        "node_modules"
    ],
    "moduleFileExtensions": [
        "js",
        "ts"
    ],
    "preset": "ts-jest",
    "testEnvironment": "node",
    "roots": [
        "<rootDir>/__tests__"
    ],
    "transform": {
        "^.+\\.(ts|tsx)$": "ts-jest"
    }
}