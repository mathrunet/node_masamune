# Marketing Pipeline Test Environment Variables
# Copy this file to .env and fill in your actual values

# ===========================================
# Google Cloud / Firebase
# ===========================================
GCP_PROJECT_ID=your-project-id
GOOGLE_SERVICE_ACCOUNT_PATH=./your-service-account.json

# ===========================================
# Google Play Developer API
# ===========================================
GOOGLE_PLAY_PACKAGE_NAME=com.example.yourapp
GOOGLE_PLAY_SERVICE_ACCOUNT_PATH=./your-service-account.json

# ===========================================
# Firebase Analytics Data API
# ===========================================
# Format: properties/123456789
FIREBASE_ANALYTICS_PROPERTY_ID=properties/your-property-id

# ===========================================
# GitHub API
# ===========================================
# Personal Access Token (Classic or Fine-grained)
# Required scopes: repo (or public_repo), read:org
GITHUB_TOKEN=ghp_xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx
GITHUB_REPO=owner/repo

# ===========================================
# App Store Connect API
# ===========================================
# Get from App Store Connect > Users and Access > Keys
APP_STORE_KEY_ID=XXXXXXXXXX
APP_STORE_ISSUER_ID=xxxxxxxx-xxxx-xxxx-xxxx-xxxxxxxxxxxx
APP_STORE_PRIVATE_KEY_PATH=./AuthKey_XXXXXXXXXX.p8
# Apple ID of your app (found in App Store Connect > App Information)
APP_STORE_APP_ID=1234567890
# Vendor Number (found in Sales and Trends or Agreements, Tax, and Banking)
APP_STORE_VENDOR_NUMBER=12345678

# ===========================================
# Cloud Storage
# ===========================================
STORAGE_BUCKET=your-project-id.appspot.com

# ===========================================
# AI / Vertex AI
# ===========================================
GEMINI_MODEL=gemini-2.0-flash
