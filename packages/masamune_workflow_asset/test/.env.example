# Asset Generation Test Environment Variables
# Copy this file to .env and fill in your actual values

# ===========================================
# Google Cloud / Firebase
# ===========================================
GCP_PROJECT_ID=your-project-id
GOOGLE_SERVICE_ACCOUNT_PATH=./your-service-account.json

# ===========================================
# Cloud Storage
# ===========================================
# Storage bucket for generated assets
STORAGE_BUCKET=your-project-id.appspot.com

# ===========================================
# Google Cloud Region
# ===========================================
# Region for Vertex AI and Cloud Run Jobs
GCLOUD_REGION=us-central1

# ===========================================
# Gemini Image Generation
# ===========================================
# Model for image generation
# Available models: gemini-2.5-flash-image, gemini-3-pro-image-preview, etc.
GEMINI_IMAGE_MODEL=gemini-2.5-flash-image

# ===========================================
# Cost Calculation (Optional)
# ===========================================
# Pricing per token for cost estimation
# Default values are used if not specified
MODEL_INPUT_PRICE=0.0000003
MODEL_OUTPUT_PRICE=0.0000025
