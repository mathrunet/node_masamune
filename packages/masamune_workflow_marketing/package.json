{
  "name": "@mathrunet/masamune_workflow_marketing",
  "version": "3.7.1",
  "description": "Masamune framework package plugin that manages marketing tools for Masamune Workflow.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "prepare": "npm run build",
    "prepublish": "npm install && npm run build && npm run test",
    "test": "NODE_OPTIONS='--experimental-vm-modules' jest",
    "cleanup": "npx ts-node scripts/cleanup_test_data.ts",
    "postversion": "npm run changelog",
    "changelog": "conventional-changelog -p eslint -i CHANGELOG.md -s -r 0 && git add CHANGELOG.md && git commit -m 'chore(release): publish package'",
    "publish": "npm publish",
    "minor_update": "npm version minor",
    "major_update": "npm version major",
    "patch_update": "npm version patch"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mathrunet/node_masamune.git"
  },
  "keywords": [
    "masamune",
    "katana",
    "flutter",
    "firebase",
    "functions"
  ],
  "author": "mathru",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mathrunet/node_masamune/issues"
  },
  "homepage": "https://github.com/mathrunet/node_masamune#readme",
  "dependencies": {
    "@google-analytics/data": "^4.9.0",
    "@google/genai": "^1.30.0",
    "@mathrunet/masamune": "^3.9.5",
    "@mathrunet/masamune_workflow": "^3.8.6",
    "firebase-admin": "^13.1.0",
    "firebase-functions": "^6.3.2",
    "googleapis": "^140.0.0",
    "jsonwebtoken": "^9.0.2",
    "pdfkit": "^0.15.0",
    "typescript": "^5.9.2"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/pdfkit": "^0.13.8",
    "conventional-changelog-cli": "^5.0.0",
    "dotenv": "^16.4.5",
    "firebase-functions-test": "^3.4.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.2"
  }
}
