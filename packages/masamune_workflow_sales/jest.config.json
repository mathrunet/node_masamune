{
    "moduleNameMapper": {
        "^puppeteer-core$": "<rootDir>/test/__mocks__/puppeteer-core.ts",
        "^@sparticuz/chromium$": "<rootDir>/test/__mocks__/@sparticuz/chromium.ts"
    },
    "moduleDirectories": [
        "src",
        "node_modules"
    ],
    "moduleFileExtensions": [
        "js",
        "ts"
    ],
    "preset": "ts-jest",
    "testEnvironment": "node",
    "roots": [
        "<rootDir>/test"
    ],
    "transform": {
        "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "setupFiles": [
        "<rootDir>/test/setup.ts"
    ],
    "globalTeardown": "<rootDir>/test/globalTeardown.ts",
    "testTimeout": 120000
}